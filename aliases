#!/bin/bash

# Autobash
alias srcbash='source ~/.bashrc'
alias atom-bash='atom ~/.bashrc ~/.bash_aliases'
alias atom-bashaliases='atom ~/.bash_aliases'
alias atom-bashrc='atom ~/.bashrc'
alias anaconda2='bash --rcfile ~/.conda_bashrc2'
alias anaconda3='bash --rcfile ~/.conda_bashrc3'

function open-nautilus() {
    PWD=$(pwd)
    echo "Nautilus into $PWD"
    nautilus $PWD
}
alias nauty='open-nautilus'

alias doosh='du -sh'
alias doosh-list='du -h --max-depth=1'
alias doosh-list-sort='du -h --max-depth=1| sort -hrk1'

PROJECTS_HOME="$( dirname "$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )" )"

# Autobash User Section
alias gows-go-test='export GOPATH=$GOPATH:/root/projects/group-go/go-test'

# Autobash - End
